# Bearable AI Health Coach - Environment Variables
# Copy this to .env and fill in your API keys

# OpenAI API Key for real AI responses
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Claude API Key (if you prefer Claude)
REACT_APP_CLAUDE_API_KEY=your_claude_api_key_here

# App Configuration
REACT_APP_APP_NAME=Bearable Health Coach
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Voice Configuration
REACT_APP_VOICE_ENABLED=true
REACT_APP_DEFAULT_VOICE=female
REACT_APP_SPEECH_RATE=1.0